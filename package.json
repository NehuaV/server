{
  "name": "app",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "NODE_ENV=production bun build src/index.ts --outdir dist --target bun",
    "prestart": "bun run build",
    "start": "NODE_ENV=production bun run dist/index.js",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "qa": "bunx biome check --write .",
    "qa:check": "bunx biome check ."
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "^0.1.7",
    "@electric-sql/pglite": "^0.2.15",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/eden": "^1.2.0",
    "@elysiajs/swagger": "^1.2.0",
    "@grotto/logysia": "^0.1.6",
    "@t3-oss/env-core": "^0.11.1",
    "better-auth": "^1.1.14",
    "drizzle-kit": "^0.30.2",
    "drizzle-orm": "^0.38.4",
    "drizzle-typebox": "^0.2.1",
    "elysia": "^1.2.10",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/bun": "^1.1.16",
    "bun-types": "latest"
  },
  "overrides": {
    "@sinclair/typebox": "^0.34.13"
  },
  "module": "src/index.js"
}
